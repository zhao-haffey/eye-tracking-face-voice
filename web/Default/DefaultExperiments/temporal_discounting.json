{"procedure":"Procedure.csv","stimuli":"Stimuli.csv","cond_array":[["name","notes","stimuli","procedure","login","download_at_end","participant_id","completion_code","start_message","end_message","fullscreen","buffer",null],["Condition1","You can put more detailed notes in this field","Stimuli.csv","Procedure.csv","on","on","random","on","","","off",5,null],[null,null,null,null,null,null,null,null,null,null,null,null,null]],"cond_loc":"https://dl.dropbox.com/s/tmg3e80od7avxxl/Conditions.csv?dl=0","all_procs":{"Procedure.csv":[["Item","trial type","Max Time","Text","Shuffle 1","delays",null],["0","temporal_discounting","user","Thank you for participating in this experiment. The task will begin during the next trial.","off","3 Weeks|3 Months|9 Months|18 Months|3 Years",null],[null,null,null,null,null,null,null]]},"all_stims":{"Stimuli.csv":[["Cue","Answer",null],["A","Apple",null],["B","Banana",null],[null,null,null]]},"conditions":[{"name":"Condition1","notes":"You can put more detailed notes in this field","stimuli":"Stimuli.csv","procedure":"Procedure.csv","login":"on","download_at_end":"on","participant_id":"on","completion_code":"on","start_message":"","end_message":"","fullscreen":"off","buffer":"5","":"","download":"on"},{"name":"","notes":"","stimuli":"","procedure":"","login":"","download_at_end":"","participant_id":"","completion_code":"","start_message":"","end_message":"","fullscreen":"","buffer":"","":"","download":"on"}],"parsed_procs":{"Procedure.csv":[{"trial type":"temporal_discounting","delays":"3 Weeks|3 Months|9 Months|18 Months|3 Years","":"","item":"0","max time":"user","text":"Thank you for participating in this experiment. The task will begin during the next trial.","shuffle 1":"off"},{"trial type":"","delays":"","":"","item":"","max time":"","text":"","shuffle 1":""}]},"surveys":{},"trialtypes":{"instruct":"<!--\r\n/*  Collector (Garcia, Kornell, Kerr, Blake & Haffey)\r\n    A program for running experiments on the web\r\n    Copyright 2012-2016 Mikey Garcia & Nate Kornell\r\n\r\n\r\n    This program is free software: you can redistribute it and/or modify\r\n    it under the terms of the GNU General Public License version 3 as published by\r\n    the Free Software Foundation.\r\n\r\n    This program is distributed in the hope that it will be useful,\r\n    but WITHOUT ANY WARRANTY; without even the implied warranty of\r\n    MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the\r\n    GNU General Public License for more details.\r\n\r\n    You should have received a copy of the GNU General Public License\r\n    along with this program.  If not, see <http://www.gnu.org/licenses/>\r\n \t\t\r\n\t\tKitten/Cat release (2019-2021) author: Dr. Anthony Haffey (a.haffey@reading.ac.uk)\t\t\r\n*/\r\n-->\r\n<style>\r\n#instruct_text{\r\n  margin:auto;\r\n  width:500px;\r\n  padding:50px;\r\n  max-height:600px;\r\n  text-align: center;  \r\n}\r\n#proceed_button{\r\n  position:absolute;\r\n  right: 20px;\r\n}\r\n#trial_contents{\r\n  position: absolute;\r\n  top:0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  margin:auto;\r\n  width:600;\r\n  height:600;\r\n}\r\n</style>\r\n<div id=\"trial_contents\">\r\n  <div id=\"instruct_text\">{{text}}</div>\r\n  <div>\r\n    <button class=\"btn btn-primary\" id=\"proceed_button\" onclick=\"Trial.submit()\">Proceed</button>\r\n  </div>\r\n</div>","temporal_discounting":"<style>\n#instruct_div{\n  width: 400px;\n  height: 600px;\n  position: absolute;\n  top:0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  margin: auto;\n}\n.choice_btn{\n  display:none;\n  width:100%;\n}\n</style>\n<div id=\"instruct_div\">\n  <h2 class=\"text-primary\"  style=\"text-align: center;padding:10px\"><b>\n    Would you prefer</b>\n    <br><br>\n    <div id=\"left_span\"></div> \n    <br>\n    <div>or</div>\n    <br>\n    <div id=\"right_span\"></div>\n  </h2>\n  <table style=\"margin: 0 auto;\">\n    <tr>\n      <td><button class=\"btn btn-primary btn-lg choice_btn\" id=\"left_response\" >Option 1</button></td>\n    </tr>\n    <tr>\n      <td><br><br></td>\n    </tr>\n    <tr>\n      <td><button class=\"btn btn-primary btn-lg choice_btn\" id=\"right_response\">Option 2</button>  </td>\n    </tr>\n    \n  </table>\n</div>\n\n<input type=\"hidden\" id=\"results_json\" name=\"results_json\"/>\n<input type=\"hidden\" id=\"auc_total\" name=\"auc_total\"/>\n<div id=\"results_div\"></div>\n\n<div id=\"testing_div\" style=\"display:none\"></div>\n\n<script>\nvar delays = \"{{delays}}\".split(\"|\");\n\nresult_object = {\n  incomplete : delays,\n  now_amount : -1\n};\n\n\nvar results_div_html = \"\";\nresult_object.incomplete.forEach(function(delay,index){\n  result_object[delay] = {\n    uu: 100,\n    ul: 100,\n    lu: 0,\n    ll: 0\n  }\n  results_div_html += \"<input type='hidden' id='delay\"+index+\"' name='delay_\"+index+\"'/>\"\n});\n$(\"#results_div\").html(results_div_html);\n\n$(\".choice_btn\").on(\"click\",function(){\n  $(\".choice_btn\").hide();\n});\n\n\nfunction prepare_question(){\n  //randomly select one of the incomplete delays\n  var left_right_now = [\"left\",\"right\"][Math.floor(Math.random() * 2)];\n  result_object.delay      = result_object.incomplete[Math.floor(Math.random() * result_object.incomplete.length)];\n  \n  \n  var upper_limit    = result_object[result_object.delay].uu;\n  var lower_limit    = result_object[result_object.delay].ll;\n  \n  //random number between the upper and lower limit\n  \n  result_object.now_amount = randomIntFromInterval(lower_limit + 1,upper_limit - 1);\n  \n  \n  \n  if(left_right_now == \"left\"){\n    $(\"#left_span,#left_response\").html(\"£\" + result_object.now_amount + \" now\");\n    $(\"#right_span,#right_response\").html(\"£100 in \" + result_object.delay);\n  }\n  if(left_right_now == \"right\"){\n    $(\"#left_span,#left_response\").html(\"£100 in \" + result_object.delay);\n    $(\"#right_span,#right_response\").html(\"£\" + result_object.now_amount + \" now\");\n  }\n  setTimeout(function(){\n    $(\".choice_btn\").show();\n  },3000);\n}\n\n\nTrial.set_timer(function(){\n  prepare_question();\n},0);\n\n\n//solution by Francisc at https://stackoverflow.com/questions/4959975/generate-random-number-between-two-numbers-in-javascript\nfunction randomIntFromInterval(min, max) { // min and max included \n  return Math.floor(Math.random() * (max - min + 1) + min);\n}\n\n$(\".choice_btn\").on(\"click\",function(){\n  var response = $(this).html();\n  var this_limits = result_object[result_object.delay];\n    \n  if(response.indexOf(\"£100\") == -1){ //they chose the sooner option\n    //bring down the upper limits accordingly\n    \n    if(result_object.now_amount < this_limits.uu & \n      result_object.now_amount >= this_limits.ul){  //if below uu but above ul then just bring down uu\n      this_limits.uu = result_object.now_amount;\n    } else if(result_object.now_amount < this_limits.ul){ //if below both uu and ul then bring uu to ul and ul to now amount\n      this_limits.uu = this_limits.ul;\n      this_limits.ul = result_object.now_amount;\n    } \n    if(result_object.now_amount < this_limits.lu){  //i.e. they choose now when now is lower than their lower upper limit\n      this_limits.lu = this_limits.ll; //reset the lower upper limit to the lower lower limit\n      this_limits.ll = 0;\n    }\n  } else { //they chose the later option\n    if(result_object.now_amount > this_limits.ll & \n       result_object.now_amount <= this_limits.lu){  //if above ll but below lu then just bring up ll\n      this_limits.ll = result_object.now_amount;\n    } else if(result_object.now_amount > this_limits.lu){ //if above both lu and ll then bring ll to lu and lu to now amount\n      this_limits.ll = this_limits.lu;\n      this_limits.lu = result_object.now_amount;\n    } \n    if(result_object.now_amount > this_limits.ul){  //i.e. they choose now when now is lower than their lower upper limit\n      this_limits.ul = this_limits.uu; //reset the lower upper limit to the lower lower limit\n      this_limits.uu = 100;\n    }\n  }\n  if(this_limits.uu - this_limits.ll <= 5){\n    var delays = \"{{delays}}\".split(\"|\");\n    var index = result_object.incomplete.indexOf(result_object.delay);\n    if (index !== -1){\n      var result_index = delays.indexOf(result_object.delay);                   //identify index of this delay \n      var result_id    = 'delay' + result_index;                                \n      var poi          = (this_limits.ll + this_limits.uu)/2;\n      $(\"#\" + result_id).val(poi);\n      result_object.incomplete.splice(index, 1);                                //remove this delay from the list of incomplete delays\n    } \n  }\n  $(\"#testing_div\").html(JSON.stringify(result_object));\n  $(\"#results_json\").val(JSON.stringify(result_object));\n  if(result_object.incomplete.length > 0){\n    prepare_question();\n  } else {\n    backup_total = 0;\n    console.dir(\"backup_total\");\n    console.dir(backup_total);\n    var delays = \"{{delays}}\".split(\"|\");\n    delays.forEach(function(delay){\n      backup_total += (result_object[delay].uu + result_object[delay].ll)/2;\n      console.dir(\"backup_total\");\n      console.dir(backup_total);\n    });\n    console.dir(\"backup_total\");\n    console.dir(backup_total);\n    $(\"#auc_total\").val(backup_total);\n    console.dir('$(\"#auc_total\").val()');\n    console.dir($(\"#auc_total\").val());\n    Trial.submit();\n  }\n});\n</script>","start_experiment":"<h1>Before starting</h1><div>Please be careful about what details you give. If you feel that any inappropriate details are being asked, please contact whoever asked you to participate.</div><button class='btn btn-primary' onclick='Trial.submit()'>Start</button>"},"location":"https://www.dropbox.com/s/jlv5925gmgyxgcb/temporal_discounting_example.json?dl=0","trial_no":0,"post_no":0,"this_condition":{"name":"Condition1","notes":"You can put more detailed notes in this field","stimuli":"Stimuli.csv","procedure"