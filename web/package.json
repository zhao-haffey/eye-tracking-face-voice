{
  "name": "Collector",
  "version": "3.0.1",
  "description": "An Open-Source package for online research",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "repository": "https://github.com/some-open-solutions/collector",
  "keywords": [
    "Online Research",
    "quick",
    "start",
    "tutorial",
    "demo"
  ],
  "author": "Mikey Garcia, Dr. Nate Kornell, Tyson Kerr, Dr. Adam Blake, Dr. Anthony Haffey. Now developed by Scientific Open Solutions <team@sopsols.org>",
  "license": "GNU General Public License",
  "devDependencies": {
    "coffee-script": "~1.6.3",
    "electron": "^10.1.3",
    "electron-builder": "^22.9.1",
    "png-to-ico": "^2.1.1"
  },
  "build": {
    "appId": "yourappid",
    "dmg": {
      "contents": [
        {
          "x": 110,
          "y": 150
        },
        {
          "x": 240,
          "y": 150,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "extraFiles": [
      "Default"
    ],
    "files": [
      "**/*",
      "!repositories/*",
      "!User/**"
    ],
    "linux": {
      "target": [
        "AppImage",
        "deb"
      ]
    },
    "nsis": {
      "allowToChangeInstallationDirectory": true,
      "oneClick": false,
      "perMachine": false,
      "allowElevation": false
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "dependencies": {
    "@octokit/auth": "^2.0.0",
    "@octokit/auth-token": "^2.4.2",
    "@octokit/rest": "^17.11.2",
    "command-exists": "^1.2.9",
    "download-git-repo": "^3.0.2",
    "electron-context-menu": "^2.3.0",
    "eval": "^0.1.4",
    "fs": "0.0.1-security",
    "fs-extra": "^9.0.1",
    "git-clone": "^0.1.0",
    "papaparse": "^5.3.0",
    "require": "^2.4.20",
    "simple-git": "^2.20.1"
  }
}
