  "trialtypes": {
    "instruct": "<!--\n/*  Collector (Garcia, Kornell, Kerr, Blake & Haffey)\n    A program for running experiments on the web\n    Copyright 2012-2016 Mikey Garcia & Nate Kornell\n\n\n    This program is free software: you can redistribute it and/or modify\n    it under the terms of the GNU General Public License version 3 as published by\n    the Free Software Foundation.\n\n    This program is distributed in the hope that it will be useful,\n    but WITHOUT ANY WARRANTY; without even the implied warranty of\n    MERCHANTABILITY or FITNESS FOR A PARTICULAR PURPOSE.  See the\n    GNU General Public License for more details.\n\n    You should have received a copy of the GNU General Public License\n    along with this program.  If not, see <http://www.gnu.org/licenses/>\n \t\t\n\t\tKitten/Cat release (2019-2021) author: Dr. Anthony Haffey (a.haffey@reading.ac.uk)\t\t\n*/\n-->\n<style>\n#instruct_text{\n  margin:auto;\n  width:500px;\n  padding:50px;\n  max-height:600px;\n  text-align: center;  \n}\n#proceed_button{\n  position:absolute;\n  right: 20px;\n}\n#trial_contents{\n  position: absolute;\n  top:0;\n  bottom: 0;\n  left: 0;\n  right: 0;\n  margin:auto;\n  width:600;\n  height:600;\n}\n</style>\n<div id=\"trial_contents\">\n  <div id=\"instruct_text\">{{text}}</div>\n  <div>\n    <button class=\"btn btn-primary\" id=\"proceed_button\" onclick=\"Phase.submit()\">Proceed</button>\n  </div>\n</div>",
    "anagram": "<!--\r\n\r\nAuthors: Dr. Anthony Haffey (a.haffey@reading.ac.uk) & Dr. Rich Harrison (rich.harrison@reading.ac.uk)\r\n\r\n-->\r\n\r\n<style>\r\n#instructions{\r\n  font-size: 50px;\r\n}\r\n#trial_content{\r\n  position: absolute;\r\n  top:0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  margin:auto;\r\n  width:600;\r\n  height:600;\r\n}\r\n.bottomBox{\r\n  border-width:20px;\r\n  height:20px;\r\n  color:white;\r\n  border-bottom: 5px solid black;\r\n}\r\n.bottomBoxSelect{\r\n  color:red;\r\n  font-size:50px;\r\n  cursor: pointer;\r\n}\r\n.topBox{\r\n  color:red;\r\n  font-size:50px;\r\n  cursor: pointer;\r\n}\r\n.topBox:hover{\r\n  color:grey;\r\n}\r\n.topBoxUsed{\r\n  display: none;\r\n  pointer-events: none;\r\n}\r\n</style>\t\r\n\r\n<div id=\"trial_content\">\r\n  <div id=\"instructions\">{{text}}</div>\r\n  <div id=\"letter_spaces\"></div>\r\n  <script>\r\n  var anagram = \"{{cue}}\".split(\"|\");\r\n  anagram.forEach(function(letter,letter_no){\r\n    $(\"#letter_spaces\").append(\"<span id='char\" + letter_no + \"' class='topBox' onclick='topLetterClick(\" + letter_no + \")'>\" + letter + \"</span>\");\r\n  });\r\n  </script>\r\n  <br><br>\r\n  <div id=\"bottomRow\"></div>\r\n  <br>\r\n  <br>\r\n  <div>\r\n    <button class=\"btn btn-primary\" onclick=\"Phase.submit()\">Next</button>\r\n  </div>\r\n</div>\r\n<textarea id=\"answerTextArea\" name=\"Response\" style=\"display:none\">No Response</textarea>\r\n\r\n<script>\r\nvar emptyPos=0;\r\nvar answerArray=[];\r\n\r\nfor(var i=0; i < 5; i++){\r\n  $(\"#bottomRow\").append(\"<span class='bottomBox' id='resp\" + i + \"' onclick='bottomLetterClick(\" + i + \")'>&nbsp&nbsp&nbsp</span>\");\r\n}\r\n\r\nfunction bottomLetterClick(y){\r\n  document.getElementById(\"resp\"+y).innerHTML='&nbsp&nbsp&nbsp';\r\n  document.getElementById(\"resp\"+y).className=\"bottomBox\";\r\n  topPos=anagram.indexOf(answerArray[y]);\r\n  document.getElementById(\"char\"+topPos).className=\"topBox\";\r\n  answerArray[y]=null;\r\n  emptyPosFind();\r\n}\r\nfunction emptyPosFind(){\r\n  for (i=0; i<5; i++){\r\n    if(answerArray[i] == null){\r\n      break;\r\n    }\r\n  }\r\n  emptyPos=i;\r\n  updateAnswer();\r\n}\r\nfunction topLetterClick(x){\r\n  document.getElementById(\"resp\"+emptyPos).innerHTML = document.getElementById(\"char\"+x).innerHTML;\r\n  answerArray[emptyPos]=document.getElementById(\"char\"+x).innerHTML;\r\n  document.getElementById(\"resp\"+emptyPos).className=\"bottomBoxSelect\";\r\n  document.getElementById(\"char\"+x).className=\"topBoxUsed\";\r\n  emptyPosFind();\r\n}\r\n\r\n\r\nfunction updateAnswer(){\r\n  var answerText=answerArray.join();\r\n  answerText=answerText.replace(/,/g,\"\");\r\n  answerTextArea.value=answerText;    \r\n}\r\n</script>",
    "anagram_speedy": "<!--\r\n\r\nAuthors: Dr. Anthony Haffey (a.haffey@reading.ac.uk) & Dr. Rich Harrison (rich.harrison@reading.ac.uk)\r\n\r\n-->\r\n\r\n<style>\r\n#instructions{\r\n  font-size: 50px;\r\n}\r\n#timer{\r\n  position:absolute;\r\n  bottom:0px;\r\n  font-size:40px;\r\n  color:blue;\r\n  right:0px;\r\n}\r\n#trial_content{\r\n  position: absolute;\r\n  top:0;\r\n  bottom: 0;\r\n  left: 0;\r\n  right: 0;\r\n  margin:auto;\r\n  width:600;\r\n  height:600;\r\n}\r\n.bottomBox{\r\n  border-width:20px;\r\n  height:20px;\r\n  color:white;\r\n  border-bottom: 5px solid black;\r\n  width:60px;\r\n  display: inline-block;\r\n}\r\n.bottomBoxSelect{\r\n  color:red;\r\n  font-size:50px;\r\n  width:60px;\r\n  cursor: pointer;\r\n  display: inline-block;\r\n}\r\n.topBox{\r\n  color:red;\r\n  font-size:50px;\r\n  width:60px;\r\n  cursor: pointer;\r\n  display: inline-block;\r\n}\r\n.topBox:hover{\r\n  color:grey;\r\n}\r\n.topBoxUsed{\r\n  display: none;\r\n  pointer-events: none;\r\n}\r\n</style>\t\r\n\r\n<div id=\"trial_content\">\r\n  <div id=\"instructions\">{{text}}</div>\r\n  <div id=\"timer\"></div>\r\n  <div id=\"letter_spaces\"></div>\r\n  <script>\r\n  var anagram = \"{{cue}}\".split(\"|\");\r\n  anagram.forEach(function(letter,letter_no){\r\n    $(\"#letter_spaces\").append(\"<span id='char\" + letter_no + \"' class='topBox' onclick='topLetterClick(\" + letter_no + \")'>\" + letter + \"</span>\");\r\n  });\r\n  </script>\r\n  <br><br>\r\n  <div id=\"bottomRow\"></div>\r\n  <br>\r\n  <br>\r\n  <div>\r\n    <button class=\"btn btn-primary\" onclick=\"check_response()\">Next</button>\r\n  </div>\r\n</div>\r\n<textarea id=\"answerTextArea\" name=\"response\" style=\"display:none\">No Response</textarea>\r\n\r\n\r\n<script>\r\nvar emptyPos=0;\r\nvar answerArray=[];\r\n\r\nfor(var i=0; i < 5; i++){\r\n  $(\"#bottomRow\").append(\"<span class='bottomBox' id='resp\" + i + \"' onclick='bottomLetterClick(\" + i + \")'>&nbsp&nbsp&nbsp</span>\");\r\n}\r\n\r\nfunction bottomLetterClick(y){\r\n  document.getElementById(\"resp\"+y).innerHTML='&nbsp&nbsp&nbsp';\r\n  document.getElementById(\"resp\"+y).className=\"bottomBox\";\r\n  topPos=anagram.indexOf(answerArray[y]);\r\n  document.getElementById(\"char\"+topPos).className=\"topBox\";\r\n  answerArray[y]=null;\r\n  emptyPosFind();\r\n}\r\nfunction emptyPosFind(){\r\n  for (i=0; i<5; i++){\r\n    if(answerArray[i] == null){\r\n      break;\r\n    }\r\n  }\r\n  emptyPos=i;\r\n  updateAnswer();\r\n}\r\nfunction topLetterClick(x){\r\n  document.getElementById(\"resp\"+emptyPos).innerHTML = document.getElementById(\"char\"+x).innerHTML;\r\n  answerArray[emptyPos]=document.getElementById(\"char\"+x).innerHTML;\r\n  document.getElementById(\"resp\"+emptyPos).className=\"bottomBoxSelect\";\r\n  document.getElementById(\"char\"+x).className=\"topBoxUsed\";\r\n  emptyPosFind();\r\n}\r\n\r\n\r\nfunction updateAnswer(){\r\n  var answerText=answerArray.join();\r\n  answerText=answerText.replace(/,/g,\"\");\r\n  answerTextArea.value=answerText;    \r\n}\r\n\r\nfunction check_response(){\r\n  if(\"{{answer}}\" == $(\"#answerTextArea\").val()){\r\n    Phase.set(\"time_limit\",this_time_limit - {{reduce_time}});\r\n  } \r\n  Phase.submit();\r\n}\r\n\r\nvar this_time_limit;\r\nvar time_remaining;\r\n\r\nPhase.set_timer(function(){\r\n  try {\r\n    this_time_limit = Phase.get(\"time_limit\");\r\n  } catch(err){\r\n    this_time_limit = {{starting_time_limit}};\r\n    Phase.set(\"time_limit\",this_time_limit);\r\n  }  \r\n},0)\r\n\r\n\r\nPhase.set_timer(function(){\r\n    \r\n  var time_remaining = this_time_limit;\r\n  setInterval(function(){\r\n    time_remaining = time_remaining - 100;\r\n    $(\"#timer\").html(time_remaining);\r\n  },100);  \r\n  setTimeout(function(){\r\n    Phase.submit();\r\n  },this_time_limit);\r\n},0);\r\n</script>"
